<MudDialog>
    <DialogContent>
        Escolha uma alternativa como correta!
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Submit" Color="Color.Primary">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    private MudDialogInstance? MudDialog { get; set; }

    private void Submit( ) => MudDialog!.Close();

}
